# Download API Configuration
DOWNLOAD_DIR=./.downloads

# Plex Integration
PLEX_URL=
PLEX_TOKEN=
PLEX_LIBRARY=Home Videos

# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USER=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_QUEUE=download_requests
RABBITMQ_VHOST=/
RABBITMQ_USE_SSL=false

# Error logging
ERROR_LOG_FILE=download_errors.log

# CloudAMQP Example (Uncomment and replace values)
# RABBITMQ_HOST=yourinstance.rmq.cloudamqp.com
# RABBITMQ_PORT=5671
# RABBITMQ_USER=youruser
# RABBITMQ_PASSWORD=yourpassword
# RABBITMQ_VHOST=yourvhost
# RABBITMQ_USE_SSL=true
